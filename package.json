{
  "name": "rockstat-website",
  "bin": {
    "rockstat": "./rockstat"
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=1 next build && cat .next/stats.txt",
    "start:dev": "node server.js",
    "start:serve": "serve",
    "start": "NODE_ENV=production node server.js",
    "clean": "rimraf public && rimraf .next",
    "build:prepare": "yarn clean && rockstat build-toc",
    "build:dev": "yarn build:prepare && next build",
    "build": "yarn build:prepare && NODE_ENV=production next build",
    "build:export": "yarn build && yarn export",
    "export": "next export -o ./public",
    "export:zeit": "MAIN=1 next export -o /public",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "classnames": "^2.2.5",
    "fs-extra": "^7.0.0",
    "micro": "^9.3.3",
    "micro-redirect": "^1.0.0",
    "mkdirp": "^0.5.1",
    "next": "^7.0.0",
    "next-routes": "^1.4.2",
    "nprogress": "^0.2.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-visibility-sensor": "^5.0.2",
    "react-youtube": "^7.9.0",
    "refractor": "^2.6.2",
    "rehype": "^7.0.0"
  },
  "devDependencies": {
    "@mapbox/rehype-prism": "^0.3.0",
    "@mdx-js/loader": "^0.16.6",
    "@mdx-js/mdx": "^0.16.6",
    "@mdx-js/tag": "^0.16.6",
    "@svgr/cli": "^4.1.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-mdx": "^1.2.0",
    "ansi-html": "0.0.7",
    "cross-env": "^5.2.0",
    "docz": "^0.13.7",
    "docz-theme-default": "^0.13.7",
    "dsv-loader": "^2.0.0",
    "globby": "^8.0.1",
    "js-yaml": "^3.13.1",
    "mdx-deck": "^1.8.2",
    "mermaid": "^8.11.0",
    "mermaid.cli": "^0.5.1",
    "npm-which": "^3.0.1",
    "postcss-advanced-variables": "^3.0.0",
    "postcss-atroot": "^0.1.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^4.1.0",
    "postcss-preset-env": "^6.1.1",
    "postcss-property-lookup": "^2.0.0",
    "postcss-rem": "^1.1.1",
    "prop-types": "^15.6.1",
    "remark-autolink-headings": "^5.1.0",
    "remark-custom-blocks": "^2.2.3",
    "remark-emoji": "^2.0.2",
    "remark-images": "^0.16.1",
    "remark-slug": "^5.1.1",
    "remark-unwrap-images": "^0.1.0",
    "rimraf": "^2.6.2",
    "serve": "^10.0.2",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-declaration-use-variable": "^1.7.0",
    "webpack-bundle-size-analyzer": "^3.0.0",
    "yargs": "^12.0.5",
    "yarn-upgrade-all": "^0.5.0"
  }
